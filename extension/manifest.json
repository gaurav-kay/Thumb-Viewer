{
      "manifest_version": 2,
      "name": "YouTube Thumbnail Viewer",
      "version": "1.0",
      "description": "Displays the thumbnail for the current video",
      "browser_action": {
            "default_popup": "popup.html",
            "default_title": "Click to view the thumbnail",
            "default_icon": "logo.png"
      },
      "permissions": [
            "tabs"
      ]
}